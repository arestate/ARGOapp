<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.ar_go.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />


    <fragment
        android:id="@+id/navigation_inquiry"
        android:name="com.example.ar_go.ui.tools.ToolsFragment"
        android:label="My Enquiries"
        tools:layout="@layout/fragment_tools" />
    <fragment
        android:id="@+id/navigation_feedback"
        android:name="com.example.ar_go.ui.gallery.GalleryFragment"
        android:label="Feedback"
        tools:layout="@layout/fragment_gallery" />


    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.ar_go.ui.share.ShareFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_share" />
</navigation>